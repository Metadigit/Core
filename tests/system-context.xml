					<item key="users"	>sys_users</item>
					<item key="groups"	>sys_groups</item>
					<item key="u2g"		>sys_users_2_groups</item>
				</arg>
			</constructor>
		</object>
		<object id="system.Mock" class="mock\GlobalMock">
